        uploadInfo.addClass("text-warning").text("上传的文件不能为空");
        return;
      }

      uploadInfo.text("上传中...");

      $.ajaxFileUpload(
        {
          url: '/citygovproj/upload',
          secureuri: false,
          fileElementId: fileElementId,
          data: fileInfo,
          dataType: 'json',
          success: function (result) {
