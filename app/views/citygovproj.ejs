                                        <th>项目名称
                                        </th>
                                        <th>项目内容
                                        </th>
                                        <th>工程地址
                                        </th>
                                        <th>操作
                                        </th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <% citygovprojs.forEach(function (citygovproj, index) { %>
                                    <tr>
                                        <td><input type="checkbox" name="selectOne" id="<%= citygovproj._id%>"/></td>
                                       <!--  <td><%= index %></td> -->
                                        <td><%if(citygovproj.basicInfo.serialno === null || citygovproj.basicInfo.serialno === undefined){%>
                                            <!--留白，不显示-->
                                            <%}else{ %>
                                            <%= citygovproj.basicInfo.serialno%>
                                            <%}%></td>
                                        <td><%if(citygovproj.basicInfo.isstatis === null || citygovproj.basicInfo.isstatis === undefined){%>
                                            <!--留白，不显示-->
                                            <%}else{ %>
                                            <%= citygovproj.basicInfo.isstatis%>
                                            <%}%></td>
