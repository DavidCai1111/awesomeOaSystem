                                            <!--留白，不显示-->
                                            <%}else{ %>
                                            <%= citygovproj.basicInfo.controlid%>
                                            <%}%>
                                        </td>
                                        <td><%if(citygovproj.basicInfo.projname === null || citygovproj.basicInfo.projname === undefined){%>
                                            <!--留白，不显示-->
                                            <%}else{ %>
                                            <%= citygovproj.basicInfo.projname%>
                                            <%}%>
                                        </td>
                                        <td><%if(citygovproj.basicInfo.firstpartyofficer === null || citygovproj.basicInfo.firstpartyofficer === undefined){%>
                                            <!--留白，不显示-->
                                            <%}else{ %>
                                            <%= citygovproj.basicInfo.firstpartyofficer%>
                                            <%}%>
                                        </td>
                                        <td><%if(citygovproj.basicInfo.projtype === null || citygovproj.basicInfo.projtype === undefined){%>
                                            <!--留白，不显示-->
                                            <%}else{ %>
                                            <%= citygovproj.basicInfo.projtype%>
                                            <%}%>
                                        </td>
                                        <td><%if(citygovproj.basicInfo.properties === null || citygovproj.basicInfo.properties === undefined){%>
                                            <!--留白，不显示-->
                                            <%}else{ %>
                                            <%= citygovproj.basicInfo.properties%>
                                            <%}%>
                                        </td>
                                        <td><%if(citygovproj.basicInfo.subname === null || citygovproj.basicInfo.subname === undefined){%>
                                            <!--留白，不显示-->
                                            <%}else{ %>
                                            <%= citygovproj.basicInfo.subname%>
                                            <%}%>
                                        </td>
                                        <td><%if(citygovproj.basicInfo.content === null || citygovproj.basicInfo.content === undefined){%>
                                            <!--留白，不显示-->
                                            <%}else{ %>
                                            <%= citygovproj.basicInfo.content%>
