<%- include header %>
<!-- MAIN PANEL -->
<div id="main" role="main">

  <!-- RIBBON -->
  <div id="ribbon">

        <span class="ribbon-button-alignment">
            <span id="refresh" class="btn btn-ribbon" data-title="refresh"
                  rel="tooltip" data-placement="bottom"
                  data-original-title="<i class='text-warning fa fa-warning'></i>
            Warning! This will reset all your widget settings."
                  data-html="true"> <i class="fa fa-refresh"></i>
        </span>
    </span>

    <!-- breadcrumb -->
    <ol class="breadcrumb">
      <li>主页</li>
      <li>市政项目管理</li>
    </ol>

  </div>
  <!-- END RIBBON -->

  <!-- MAIN CONTENT -->
  <div id="content">

    <div class="row">
      <div class="col-xs-12 col-sm-7 col-md-7 col-lg-4">
        <h1 class="page-title txt-color-blueDark"><i class="fa-fw fa fa-home"></i>
          主页
          <span>> 市政项目管理</span>
        </h1>
      </div>

    </div>
    <!-- widget grid -->
    <section id="widget-grid">

      <!-- row -->
      <div class="row">
        <article class="col-sm-12">
          <!-- new widget -->
          <div class="jarviswidget jarviswidget-color-blueDark" id="wid-id-0" data-widget-editbutton="false">
            <header>
                        <span class="widget-icon">
                            <i class="fa fa-lg fa-fw fa-home"></i>
                        </span>

              <h2 id="gridTitle">市政项目管理</h2>
            </header>

            <!-- 按钮区 BEGIN -->
            <div id="ControlButton" class="hidden">
              <div style="position: absolute;left: 200px;top:4px">
                <button id="new-proj" data-toggle="modal" class="btn btn-default" href="#addProj"
                        style="position:relative">新增市政项目
                </button>
                <button id="delete-proj" class="btn btn-default" href="javascript:void(0);"
                        style="position:relative">删除市政项目
                </button>
                <a href="/citygovproj/getExcel">
                  <button id="exportToExcel" class="btn btn-default" href="javascript:void(0);"
                          style="position:relative">导出此项目Excel
                  </button>
                </a>
              </div>
            </div>

            <!-- 按钮区 END -->

            <!-- widget div-->
            <div>

              <!-- widget edit box -->
              <div class="jarviswidget-editbox">
                <!-- This area used as dropdown edit box --> </div>
              <!-- end widget edit box -->

              <!-- widget content -->
              <div class="widget-body no-padding">
                <div class="widget-body-toolbar"></div>

                <table id="datatable_col_reorder" class="table table-striped table-hover">
                  <thead>
                  <tr>
                    <th>
                      <input type="checkbox" name="selectAll"/>
                    </th>
                    <th>序号</th>
                    <th>是否统计</th>
                    <th>受控编号</th>
                    <th>工程名称</th>
                    <th>甲方项目负责人</th>
                    <th>项目类别</th>
                    <th>工程属性</th>
                    <th>项目名称</th>
                    <th>项目内容</th>
                    <th>工程地址</th>
                    <th>操作</th>
                  </tr>
                  </thead>
                  <tbody>
                  <% citygovprojs.forEach(function (citygovproj, index) { %>
                  <tr>
                    <td>
                      <input type="checkbox" name="selectOne" id="<%= citygovproj._id %>"/></td>
                    <!--  <td>
                                        <%= index %></td>
                                    -->
                    <td>
                      <% if(citygovproj.basicInfo.serialno === null || citygovproj.basicInfo.serialno === undefined){ %>
                      <!--留白，不显示-->
                      <% }else{ %>
                      <%= citygovproj.basicInfo.serialno %>
                      <% } %>
                    </td>
                    <td>
                      <% if(citygovproj.basicInfo.isstatis === null || citygovproj.basicInfo.isstatis === undefined){ %>
                      <!--留白，不显示-->
                      <% }else{ %>
                      <%= citygovproj.basicInfo.isstatis %>
                      <% } %>
                    </td>
                    <td>
                      <% if(citygovproj.basicInfo.controlid === null || citygovproj.basicInfo.controlid === undefined){ %>
                      <!--留白，不显示-->
                      <% }else{ %>
                      <%= citygovproj.basicInfo.controlid %>
                      <% } %>
                    </td>
                    <td>
                      <% if(citygovproj.basicInfo.projname === null || citygovproj.basicInfo.projname === undefined){ %>
                      <!--留白，不显示-->
                      <% }else{ %>
                      <%= citygovproj.basicInfo.projname %>
                      <% } %>
                    </td>
                    <td>
                      <% if(citygovproj.basicInfo.firstpartyofficer === null || citygovproj.basicInfo.firstpartyofficer === undefined){ %>
                      <!--留白，不显示-->
                      <% }else{ %>
                      <%= citygovproj.basicInfo.firstpartyofficer %>
                      <% } %>
                    </td>
                    <td>
                      <% if(citygovproj.basicInfo.projtype === null || citygovproj.basicInfo.projtype === undefined){ %>
                      <!--留白，不显示-->
                      <% }else{ %>
                      <%= citygovproj.basicInfo.projtype %>
                      <% } %>
                    </td>
                    <td>
                      <% if(citygovproj.basicInfo.properties === null || citygovproj.basicInfo.properties === undefined){ %>
                      <!--留白，不显示-->
                      <% }else{ %>
                      <%= citygovproj.basicInfo.properties %>
                      <% } %>
                    </td>
                    <td>
                      <% if(citygovproj.basicInfo.subname === null || citygovproj.basicInfo.subname === undefined){ %>
                      <!--留白，不显示-->
                      <% }else{ %>
                      <%= citygovproj.basicInfo.subname %>
                      <% } %>
                    </td>
                    <td>
                      <% if(citygovproj.basicInfo.content === null || citygovproj.basicInfo.content === undefined){ %>
                      <!--留白，不显示-->
                      <% }else{ %>
                      <%= citygovproj.basicInfo.content %>
                      <% } %>
                    </td>
                    <td>
                      <% if(citygovproj.basicInfo.address === null || citygovproj.basicInfo.address === undefined){ %>
                      <!--留白，不显示-->
                      <% }else{ %>
                      <%= citygovproj.basicInfo.address %>
                      <% } %>
                    </td>
                    <td>
                      <a class="edit" data-toggle="modal" href="#editProj" data-projId="<%= citygovproj._id %>">详情</a>
                    </td>
                  </tr>
                  <% }) %>
                  </tbody>
                </table>

              </div>
              <!-- end widget content --> </div>
            <!-- end widget div --> </div>
          <!-- end widget --> </article>
      </div>

      <!-- end row -->

    </section>
    <!-- end widget grid -->

    <!-- Modal 弹出框 begin-->
    <div class="modal fade" id="addProj" tabindex="-1" role="dialog widget">
      <div class="modal-dialog">
        <!-- widget grid -->
        <section id="widget-grid" class="modal-content">
          <!-- row -->
          <div class="row">
            <!-- NEW WIDGET START -->
            <article class="col-sm-12 col-md-12 col-lg-12 sortable-grid ui-sortable">
              <!-- Widget ID (each widget will need unique ID)-->
              <div class="jarviswidget jarviswidget-color-darken" id="wid-id-0" data-widget-editbutton="false"
                   data-widget-deletebutton="false" style="
    margin-bottom: 0px;">
                <!-- widget options:
                            usage: <div class="jarviswidget" id="wid-id-0" data-widget-editbutton="false">
                data-widget-colorbutton="false"
                            data-widget-editbutton="false"
                            data-widget-togglebutton="false"
                            data-widget-deletebutton="false"
                            data-widget-fullscreenbutton="false"
                            data-widget-custombutton="false"
                            data-widget-collapsed="true"
                            data-widget-sortable="false"

                            -->
                <header>
                  <div class="jarviswidget-ctrls" role="menu">
                    <a href="javascript:void(0);" class="button-icon jarviswidget-delete-btn closeBtn" rel="tooltip"
                       title="" data-placement="bottom" data-original-title="close" data-dismiss="modal">
                      <i class="fa fa-times"></i>
                    </a>
                  </div>
                  <h2>新增市政项目</h2>
                </header>

                <!-- widget div-->
                <div>

                  <!-- widget edit box -->
                  <div class="jarviswidget-editbox">
                    <!-- This area used as dropdown edit box --> </div>
                  <!-- end widget edit box -->

                  <!-- widget content -->
                  <div class="widget-body">

                    <div class="row">
                      <form id="wizard-1" novalidate="novalidate" class="smart-form">
                        <div id="bootstrap-wizard-1" class="col-sm-12">
                          <div class="form-bootstrapWizard">

                            <ul class="bootstrapWizard form-wizard">
                              <% if(user.authority.citygovprojControl.basicInfo === true){ %>
                              <li class="active" data-target="#step1"
                                  style="padding-right: 0;padding-left: 0;width: 16.67%">
                                <a href="#tab1" data-toggle="tab">
                                                            <span
                                                              class="step">1</span>
                                  <span class="title">基本信息</span>
                                </a>
                              </li>
                              <% } %>

                              <% if(user.authority.citygovprojControl.materialInfo === true){ %>
                              <li data-target="#step2"
                                <% if(user.authority.citygovprojControl.basicInfo === false){ %>
                                  class="active"
                                <% } %>
                                  style="padding-right: 0;padding-left: 0;width: 16.67%">
                                <a href="#tab2" data-toggle="tab">
                                                            <span
                                                              class="step">2</span>
                                  <span class="title">材料信息</span>
                                </a>
                              </li>
                              <% } %>

                              <% if(user.authority.citygovprojControl.constructionInfo === true){ %>
                              <li data-target="#step3"
                                <% if(user.authority.citygovprojControl.basicInfo === false && user.authority.citygovprojControl.materialInfo === false){ %>
                                  class="active"
                                <% } %>
                                  style="padding-right: 0;padding-left: 0;width: 16.67%">
                                <a href="#tab3" data-toggle="tab">
                                                            <span
                                                              class="step">3</span>
                                  <span class="title">施工情况信息</span>
                                </a>
                              </li>
                              <% } %>

                              <% if(user.authority.citygovprojControl.textInfo === true){ %>
                              <li data-target="#step4"
                                <% if(user.authority.citygovprojControl.basicInfo === false && user.authority.citygovprojControl.materialInfo === false && user.authority.citygovprojControl.constructionInfo === false){ %>
                                  class="active"
                                <% } %>
                                  style="padding-right: 0;padding-left: 0;width: 16.67%">
                                <a href="#tab4" data-toggle="tab">
                                                            <span
                                                              class="step">4</span>
                                  <span class="title">资料信息</span>
                                </a>
                              </li>
                              <% } %>

                              <% if(user.authority.citygovprojControl.contractInfo === true){ %>
                              <li data-target="#step5"
                                <% if(user.authority.citygovprojControl.basicInfo === false && user.authority.citygovprojControl.materialInfo === false && user.authority.citygovprojControl.constructionInfo === false && user.authority.citygovprojControl.textInfo === false){ %>
                                  class="active"
                                <% } %>
                                  style="padding-right: 0;padding-left: 0;width: 16.67%">
                                <a href="#tab5" data-toggle="tab">
                                                            <span
                                                              class="step">5</span>
                                  <span class="title">合同信息</span>
                                </a>
                              </li>
                              <% } %>

                              <% if(user.authority.citygovprojControl.incomeInfo === true){ %>
                              <li data-target="#step6"
                                <% if(user.authority.citygovprojControl.basicInfo === false && user.authority.citygovprojControl.materialInfo === false && user.authority.citygovprojControl.constructionInfo === false && user.authority.citygovprojControl.textInfo === false && user.authority.citygovprojControl.contractInfo === false){ %>
                                  class="active"
                                <% } %>
                                  style="padding-right: 0;padding-left: 0;width: 16.6%">
                                <a href="#tab6" data-toggle="tab">
                                                            <span
                                                              class="step">6</span>
                                  <span class="title">收入信息</span>
                                </a>
                              </li>
                              <% } %>

                            </ul>

                            <div class="clearfix"></div>
                          </div>
                          <div class="tab-content">
                            <% if(user.authority.citygovprojControl.basicInfo === true){ %>
                            <div class="tab-pane active" id="tab1">
                              <br>

                              <!-- <h3> <strong>1</strong>
                              - 基本信息
                          </h3>
                          -->
                              <fieldset>

                                <section>
                                  <div class="input-group hidden">
                                    <input class="form-control" placeholder="序号" type="text" name="serialno">
                                    <span class="input-group-addon">序号</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <label class="select">
                                      <select class="input-sm" name="isstatis">
                                        <option value="是">是</option>
                                        <option value="否">否</option>
                                      </select>
                                      <i></i>
                                    </label>
                                    <span class="input-group-addon">是否统计</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <input class="form-control" placeholder="受控编号" type="text" name="controlid">
                                    <span class="input-group-addon">受控编号</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <input class="form-control" placeholder="工程名称" type="text" name="projname">
                                    <span class="input-group-addon">工程名称</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <input class="form-control" placeholder="甲方项目负责人" type="text"
                                           name="firstpartyofficer">
                                    <span class="input-group-addon">甲方项目负责人</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <label class="select">
                                      <select class="input-sm" name="projtype">
                                        <option value="覆盖网">覆盖网</option>
                                        <option value="光交网">光交网</option>
                                      </select>
                                      <i></i>
                                    </label>
                                    <span class="input-group-addon">项目类别</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <label class="select">
                                      <select class="input-sm" name="properties">
                                        <% selectOptions.propertiesList.forEach(function (option) { %>
                                        <option value="<%= option %>"><%= option %></option>
                                        <% }) %>
                                      </select>
                                      <i></i>
                                    </label>
                                    <span class="input-group-addon">工程属性</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <input class="form-control" placeholder="项目名称" type="text" name="subname">
                                    <span class="input-group-addon">项目名称</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <input class="form-control" placeholder="项目内容" type="text" name="content">
                                    <span class="input-group-addon">项目内容</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <input class="form-control" placeholder="工程地址" type="text" name="address">
                                    <span class="input-group-addon">工程地址</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <label class="select">
                                      <select class="input-sm" name="firstparty">
                                        <% selectOptions.firstpartyList.forEach(function (option) { %>
                                        <option value="<%= option %>"><%= option %></option>
                                        <% }) %>
                                      </select>
                                      <i></i>
                                    </label>
                                    <span class="input-group-addon">甲方单位</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <label class="select">
                                      <select class="input-sm" name="area">
                                        <% selectOptions.areaList.forEach(function (option) { %>
                                        <option value="<%= option %>"><%= option %></option>
                                        <% }) %>
                                      </select>
                                      <i></i>
                                    </label>
                                    <span class="input-group-addon">所属区域</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <label class="select">
                                      <select class="input-sm" name="designorg">
                                        <% selectOptions.designorgList.forEach(function (option) { %>
                                        <option value="<%= option %>"><%= option %></option>
                                        <% }) %>
                                      </select>
                                      <i></i>
                                    </label>
                                    <span class="input-group-addon">设计单位</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <input class="form-control" placeholder="设计负责人" type="text" name="designofficer">
                                    <span class="input-group-addon">设计负责人</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <label class="select">
                                      <select class="input-sm" name="supvorg">
                                        <% selectOptions.supvorgList.forEach(function (option) { %>
                                        <option value="<%= option %>"><%= option %></option>
                                        <% }) %>
                                      </select>
                                      <i></i>
                                    </label>
                                    <span class="input-group-addon">监理单位</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <input class="form-control" placeholder="监理负责人" type="text" name="supvofficer">
                                    <span class="input-group-addon">监理负责人</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <label class="select">
                                      <select class="input-sm" name="constructteam">
                                        <% selectOptions.constructteamList.forEach(function (option) { %>
                                        <option value="<%= option %>"><%= option %></option>
                                        <% }) %>
                                      </select>
                                      <i></i>
                                    </label>
                                    <span class="input-group-addon">施工队</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <textarea rows="5" name="basiccomments" placeholder="备注" class="form-control"
                                              class="custom-scroll " style="resize: none;"></textarea>
                                    <span class="input-group-addon">备注</span>
                                  </div>
                                </section>

                              </fieldset>

                            </div>
                            <% } %>

                            <% if(user.authority.citygovprojControl.materialInfo === true){ %>
                            <div
                              class="tab-pane <% if(user.authority.citygovprojControl.basicInfo === false){ %> active <% } %>"
                              id="tab2">
                              <br>

                              <!--  <h3> <strong>2</strong>
    - 材料信息
  </h3>
  -->
                              <fieldset>
                                <section>
                                  <div class="input-group">
                                    <input class="form-control digit" placeholder="主材料使用" type="text"
                                           name="materialoffer">
                                    <span class="input-group-addon">主材料使用</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <input class="form-control digit" placeholder="主材料金额" type="text"
                                           name="materialamont">
                                    <span class="input-group-addon">主材料金额</span>
                                  </div>
                                </section>
                              </fieldset>
                            </div>
                            <% } %>

                            <% if(user.authority.citygovprojControl.constructionInfo === true){ %>
                            <div
                              class="tab-pane <% if(user.authority.citygovprojControl.basicInfo === false && user.authority.citygovprojControl.materialInfo === false){ %> active <% } %>"
                              id="tab3">
                              <br>
                              <fieldset>
                                <!-- <h3>
    <strong>3</strong>
    - 施工情况信息
  </h3>
  -->
                                <section>
                                  <div class="input-group">
                                    <textarea rows="5" name="changeoverreport" placeholder="割接报告申请" class="form-control"
                                              class="custom-scroll " style="resize: none;"></textarea>
                                    <span class="input-group-addon">割接报告申请</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <input type="text" class="form-control" placeholder="年/月/日" data-mask="9999/99/99"
                                           data-mask-placeholder="-" name="changeoverreportapplytime">
                                                    <span class="input-group-addon">
                                                        <i class="fa fa-calendar"></i>
                                                    </span>
                                    <span class="input-group-addon">割接报告申请日期</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <input type="text" class="form-control" placeholder="年/月/日" data-mask="9999/99/99"
                                           data-mask-placeholder="-" name="changeovertime">
                                                    <span class="input-group-addon">
                                                        <i class="fa fa-calendar"></i>
                                                    </span>
                                    <span class="input-group-addon">割接日期</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <input type="text" class="form-control" placeholder="年/月/日" data-mask="9999/99/99"
                                           data-mask-placeholder="-" name="begintime">
                                                    <span class="input-group-addon">
                                                        <i class="fa fa-calendar"></i>
                                                    </span>
                                    <span class="input-group-addon">开工时间</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <input type="text" class="form-control" placeholder="年/月/日" data-mask="9999/99/99"
                                           data-mask-placeholder="-" name="endtime">
                                                    <span class="input-group-addon">
                                                        <i class="fa fa-calendar"></i>
                                                    </span>
                                    <span class="input-group-addon">完工时间</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <textarea rows="5" name="constructioncomments" placeholder="备注" class="form-control"
                                              class="custom-scroll " style="resize: none;"></textarea>
                                    <span class="input-group-addon">备注</span>
                                  </div>
                                </section>
                              </fieldset>

                            </div>
                            <% } %>

                            <% if(user.authority.citygovprojControl.textInfo === true){ %>
                            <div
                              class="tab-pane <% if(user.authority.citygovprojControl.basicInfo === false && user.authority.citygovprojControl.materialInfo === false && user.authority.citygovprojControl.constructionInfo === false){ %> active <% } %>"
                              id="tab4">
                              <br>

                              <!--  <h3>
    <strong>4</strong>
    - 资料信息
  </h3>
  -->
                              <fieldset>
                                <section>
                                  <div class="input-group">
                                    <input class="form-control" placeholder="竣工文本（甲方下发）收到情况" type="text" name="text">
                                    <span class="input-group-addon">竣工文本（甲方下发）收到情况</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <input type="text" class="form-control" placeholder="年/月/日" data-mask="9999/99/99"
                                           data-mask-placeholder="-" name="textreceivetime">
                                                <span class="input-group-addon">
                                                    <i class="fa fa-calendar"></i>
                                                </span>
                                    <span class="input-group-addon">竣工文本收到日期</span>
                                  </div>
                                </section>

                                <section>
                                  <div class="input-group">
                                    <input type="text" class="form-control" placeholder="年/月/日" data-mask="9999/99/99"
                                           data-mask-placeholder="-" name="textsubmittime">
                                                <span class="input-group-addon">
                                                    <i class="fa fa-calendar"></i>
                                                </span>
                                    <span class="input-group-addon">竣工文本提交日期</span>
                                  </div>
                                </section>

