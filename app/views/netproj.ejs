        //清除输入框内表单的错误信息
        function clearErrInfo(formId){
            $("#" + formId + " span[class*='err-info']").remove();
            $("#" + formId + " input").each(function(){
                $(this)
                        .parent()
                        .parent()
                        .removeClass("has-error");
            });
        }
        function clearErrInfoByInputName(formId,inputName){
            $("#" + formId + " input[name='" + inputName + "']").each(function(){
                $(this)
                        .parent()
                        .parent()
                        .removeClass("has-error")
                        .children(".err-info")
                        .remove();
            });
        }

